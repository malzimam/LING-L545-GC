DELIMITERS = "." ;

LIST PART = PART ;
LIST PUNCT = PUNCT ;
LIST ADP = ADP AdpType=Prep Case=Acc ;
LIST PRON = PRON ;
LIST AUX = AUX ;
LIST NOUN = NOUN ;
LIST NOM = Case=Nom ;
LIST NOM1 = Case=Gen ;
LIST VERB = VERB ;
LIST CCONJ = CCONJ ;
LIST BOS = (>>>) ;
LIST eos = (<<<) ;
SECTION

REMOVE VERB IF (1C PRON) ;
REMOVE NOM IF (-1 ADP) ;
REMOVE NOM1 IF (-1 ADP) ;
SELECT CCONJ IF (-1 BOS) ;
